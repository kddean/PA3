
public class Review {
	String genre;
	String date;
	String name;
	String source;
	String title;
	String text;
	
	public Review(String genre, String date, String name, String source, String title, String text) {
		this.genre = genre;
		this.date = date;
		this.name = name;
		this.source = source;
		this.title = title;
		this.text = text;
	}

	
}
